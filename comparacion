// Compara.cpp : Defines the entry point for the console application.
//

#include "stdafx.h"
#include "iostream"
#include "conio.h"

using namespace std;


void main()
{
	int A,B,C,mayor;
	cout<< "Ingrese A ";
	cin>> A;
	cout<< "Ingrese B ";
	cin>> B;
	cout<< "Ingrese C ";
	cin>> C;
	if (A>B)
		if (A>C)
			mayor=A;
		else 
			mayor=C;
	else
		if (B>C)
			mayor=B;
		else
			mayor=C;
	cout<< "El mayor es: "<<mayor;
	getch();
}


